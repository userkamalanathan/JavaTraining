import java.awt.*;
class sample extends Frame{
    sample() {
        Choice c = new Choice();
        c.add("MALE");
        c.add("FEMALE");
        Label l = new Label("Welcome");
        add(l);
        add(c);
        setLayout(new FlowLayout());
        setVisible(true);
        setSize(1000,1000);
    }
}
class AWT{
    public static void main(String[]args){
        new sample();
    }
}
